{
    "name": "mindwingx/dpas",
    "description": "Design Patterns As Service",
    "license": "MIT",
    "authors": [
        {
            "name": "Milad Roudgarian",
            "email": "milad.rg@gmail.com",
            "homepage": "https://twitter.com/mindwingx"
        }
    ],
    "homepage": "https://github.com/mindwingx/dpas",
    "keywords": [
        "Laravel",
        "Dpas",
        "Design Patterns"
    ],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Mindwingx\\Dpas\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mindwingx\\Dpas\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mindwingx\\Dpas\\DpasServiceProvider"
            ],
            "aliases": {
                "Dpas": "Mindwingx\\Dpas\\Facades\\Dpas"
            }
        }
    }
}
